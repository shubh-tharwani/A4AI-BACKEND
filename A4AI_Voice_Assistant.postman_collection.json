{
  "info": {
    "name": "A4AI Voice Assistant API",
    "_postman_id": "a4ai-voice-assistant",
    "description": "Collection to test Voice Assistant feature in FastAPI",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Voice Assistant - Upload Audio",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <FIREBASE_ID_TOKEN>",
            "type": "text"
          }
        ],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": "/path/to/your/audio.wav"
            }
          ]
        },
        "url": {
          "raw": "http://127.0.0.1:8000/voice-assistant/",
          "protocol": "http",
          "host": [
            "127",
            "0",
            "0",
            "1"
          ],
          "port": "8000",
          "path": [
            "voice-assistant",
            ""
          ]
        }
      },
      "response": []
    }
  ]
}
